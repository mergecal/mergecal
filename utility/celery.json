{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "REMAP_SIGTERM=SIGQUIT celery -A config.celery_app worker --beat --scheduler django --loglevel=info --concurrency 2",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
